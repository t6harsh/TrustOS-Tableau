<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  <dashboard-extension id="com.trustos.safety" extension-version="1.0.0">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description>TrustOS - The "Check Engine Light" for Your Dashboard. Automatically detects data anomalies and locks dashboards to prevent decisions on bad data.</description>
    <author name="TrustOS Team" email="team@trustos.dev" organization="TrustOS" website="https://trustos.dev"/>
    <min-api-version>1.7</min-api-version>
    <source-location>
      <url>http://localhost:3000/index.html</url>
    </source-location>
    <icon></icon>
    <permissions>
      <permission>full data</permission>
    </permissions>
  </dashboard-extension>
  <resources>
    <resource id="name">
      <text locale="en_US">TrustOS Safety Layer</text>
    </resource>
  </resources>
</manifest>